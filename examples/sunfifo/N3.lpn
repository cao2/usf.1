<mod name="c3">
        <var>
	  seout3 = 1;
	  eoutb3 = 0;
	  eout3 = 1;
	  foutb3 = 1;
	  fout3 = 0;
	  sein = 1;
        </var>
        <marking> 0, 1, 2, 3, 4, 5 </marking>

	<transition label="1" preset="0" postset="0">
	   fout2 && eout3 && seout3;
           (90, 110);
	   seout3 = 0;
	</transition>
	<transition label="2" preset="0" postset="0">
	   (!fout2 || !eout3) && !seout3;
           (90, 110);
	   seout3 = 1;
	</transition>

	<transition label="3" preset="1" postset="1">
           seout3 && foutb3 && eoutb3;
	   (90, 110);
           eoutb3 = 0;
	</transition>
	<transition label="4" preset="1" postset="1">
           (!seout3 || !foutb3) && !eoutb3;
	   (90, 110);
