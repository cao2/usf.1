	</transition>
	<transition label="6" preset="2" postset="2">
           !eoutb3 && !eout3;
	   (90, 110);
           eout3 = 1;
	</transition>

	<transition label="7" preset="3" postset="3">
           eoutb3 && sein && foutb3;
           (90, 110);
           foutb3 = 0;
	</transition>
	<transition label="8" preset="3" postset="3">
           (!eoutb3 || !sein) && !foutb3;
           (90, 110);
           foutb3 = 1;
