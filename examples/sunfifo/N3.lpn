	</transition>

	<transition label="11" preset="5" postset="5">
           fout3 && sein;
	   (90, inf);
           sein = 0;
	</transition>
	<transition label="12" preset="5" postset="5">
           !fout3 && !sein;
	   (90, 110);
           sein = 1;
	</transition>
</mod>
