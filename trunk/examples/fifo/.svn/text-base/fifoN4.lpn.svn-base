<main>
	left left(c1.lo);
	cell c1(left.ro, c2.lo);
	cell c2(c1.ro, right.lo);
	right right(c2.ro);
</main>


<class name="left" arg="ri">
   <var>
   ce = 0;
   li = 0;
   lo = 0;
   nor = 1;
   ro = 0;
   </var>

   <m>
      0,   2,   4,   6,   26
   </m>

   <tr label="8" preset="0" postset="1">
     guard(li && nor);
     delay(0, inf);
     lo = 1;
   </tr>
   <tr label="9" preset="1" postset="0">
      guard(!li && !nor);
     delay(0, inf);
     lo = 0;
   </tr>
   <tr label="10" preset="2" postset="3">
      guard(!ri && ce || ri);
     delay(0, inf);
     nor = 0;
   </tr>
   <tr label="11" preset="3" postset="2">
      guard(!ri && !ce);
     delay(0, inf);
     nor = 1;
   </tr>
   <tr label="12" preset="4" postset="5">
     guard(lo&& !ri);
     delay(0, inf);
     ce = 1;
   </tr>
   <tr label="13" preset="5" postset="4">
      guard(!lo && ri);
     delay(0, inf);
     ce = 0;
   </tr>
   <tr label="14" preset="6" postset="7">
      guard(!lo && ce);
     delay(0, inf);
     ro = 1;
   </tr>
   <tr label="15" preset="7" postset="6">
      guard(!lo&& !ce || lo);
     delay(0, inf);
     ro = 0;
   </tr>
   <tr label="48" preset="26" postset="25">
      guard(!lo);
     delay(0, inf);
     li = 1;
   </tr>
   <tr label="50" preset="25" postset="26">
     guard(lo);
     delay(0, inf);
     li = 0;
   </tr>
</class>


